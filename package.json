{
    "name": "leetsound",
    "version": "0.1.0",
    "private": true,
    "description": "A JAM stack PWA, online music streaming web app built with GraphQL, Nexus schema, Apollo, React, TypeScript, Next.js, JWT and Prisma ORM. ",
    "keywords": [
        "music",
        "PWA",
        "React",
        "TypeScript",
        "Next.js",
        "GraphQL",
        "JWT",
        "next-auth"
    ],
    "author": "Kaushal",
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "generate:nexus": "ts-node --transpile-only -P nexus.ts graphql/Types",
        "migrate:prisma": "prisma migrate dev --preview-feature",
        "dbpush:prisma": "prisma db push",
        "generate:prisma": "prisma generate",
        "introspect:prisma": "prisma introspect",
        "studio:prisma": "prisma studio",
        "format:prisma": "prisma format"
    },
    "dependencies": {
        "@apollo/client": "^3.3.15",
        "@prisma/client": "^2.24.1",
        "apollo-server-micro": "^2.23.0",
        "dataloader": "^2.0.0",
        "graphql": "^15.5.0",
        "next": "10.2.3",
        "next-auth": "^3.13.2",
        "nextjs-progressbar": "^0.0.10",
        "nexus": "^1.0.0",
        "nexus-plugin-prisma": "^0.35.0",
        "react": "17.0.2",
        "react-device-detect": "^1.17.0",
        "react-dom": "17.0.2",
        "react-jinke-music-player": "^4.24.1",
        "swr": "0.5.6"
    },
    "devDependencies": {
        "@types/next-auth": "^3.7.2",
        "@types/node": "^15.0.2",
        "@types/react": "^17.0.3",
        "@typescript-eslint/eslint-plugin": "^4.18.0",
        "@typescript-eslint/parser": "^4.18.0",
        "css-loader": "^5.2.6",
        "eslint": "^7.22.0",
        "eslint-plugin-react": "^7.22.0",
        "postcss-loader": "^6.1.0",
        "prettier": "^2.2.1",
        "prisma": "^2.24.1",
        "style-loader": "^2.0.0",
        "ts-node": "^10.0.0",
        "typescript": "^4.2.3"
    }
}
